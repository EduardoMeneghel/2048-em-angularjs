  function verificacaoRight(keyValue, collumn, posicao1, posicao2, posicao3, posicao4){
    switch(keyValue){
      case 1:
        $scope.game.data[collumn]['value1'] = null;
        $scope.game.data[collumn]['value2'] = null;
        $scope.game.data[collumn]['value3'] = posicao1*2;
        $scope.game.data[collumn]['value4'] = posicao3*2;
      break;
      case 2:
        $scope.game.data[collumn]['value1'] = null;
        $scope.game.data[collumn]['value2'] = null;
        $scope.game.data[collumn]['value3'] = null;
