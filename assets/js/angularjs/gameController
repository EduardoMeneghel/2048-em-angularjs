(function(angular) {
angular.module('ngGame', []).controller('ngGameController', function($scope) {

  
  
  $scope.game = {
    "data" : {
      "column1": {
        value1:2,
        value2:2,
        value3:2,
        value4:null
      },
      "column2": {
        value1:2,
        value2:null,
        value3:2,
        value4:2
      },
      "column3": {
        value1:2,
        value2:2,
        value3:8,
        value4:null
      },
      "column4": {
        value1:null,
        value2:8,
        value3:null,
        value4:2
      },
    },
    "color" : {
        "column1": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
        "column2": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
        "column3": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
        "column4": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
      }
  };

  color();

  let valores = [];

  $scope.keyPress = function(keyEvent) {
    switch(keyEvent.which){
      case 38:
        $scope.up();
      break;
      case 40:
        $scope.down();
      break;
      case 37:
        $scope.left();
      break;
      case 39:
        $scope.right();
      break;
    }
  }

  $scope.up = function(){
    oldData = angular.copy($scope.game);
    let collumns = [];
    let positions = [];
    let contadorCollumns = 1;
    let contadorPosition = 1;
    let contador = 1;
    let valores = [];
    let obj;
    angular.forEach($scope.game.data, function(obj, collumn){
      collumns[contadorCollumns] = collumn;
      contadorCollumns++;
      let contadorPositions = 1;
      angular.forEach($scope.game.data[collumn], function(value, position){
        positions[contadorPositions] = position;
        contadorPositions++
      });
    });
    angular.forEach($scope.game.data, function(){
      contador = 1;
      angular.forEach($scope.game.data, function(){
      valores[contador] = $scope.game.data[collumns[contador]][positions[contadorPosition]];
      contador++;
      })
      //2244  1
      valores[1] == valores[2] && valores[3] == valores[4] && valores[1] && valores[3]  ? verificacaoUp(1,collumns,  valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //2200  2
      valores[1] == valores[2] && valores[1] && !valores[3] && !valores[4] ? verificacaoUp(2, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2204  3
      valores[1] == valores[2] && valores[1] && !valores[3] && valores[4] ? verificacaoUp(3, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2240  4
      valores[1] == valores[2] && valores[1] && valores[3] && !valores[4] ? verificacaoUp(4, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2248  5
      valores[1] == valores[2] && valores[1] && valores[3] && valores[4] && valores[2] != valores[3] && valores[3] != valores[4] ? verificacaoUp(5, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0022  6
      valores[3] == valores[4] && valores[3] && !valores[1] && !valores[2] ? verificacaoUp(6, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0422  7
      valores[3] == valores[4] && valores[3] && !valores[1] && valores[2] ? verificacaoUp(7, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4022  8
      valores[3] == valores[4] && valores[3] && valores[1] && !valores[2] ? verificacaoUp(8, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4822  9
      valores[3] == valores[4] && valores[3] && valores[1] && valores[2] && valores[1] != valores[2] ? verificacaoUp(9, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0220  10
      valores[2] == valores[3] && valores[2] && !valores[1] && !valores[4] ? verificacaoUp(10, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4220  11
      valores[2] == valores[3] && valores[2] && valores[1] && valores[1] != valores[2] && !valores[4] ? verificacaoUp(11, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0224  12
      valores[2] == valores[3] && valores[2] && !valores[1] && valores[4] ? verificacaoUp(12, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4224  13
      valores[2] == valores[3] && valores[2] && valores[1] && valores[2] != valores[1] && valores[4] ? verificacaoUp(13, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;


      //0202  14
      valores[2] == valores[4] && valores[2] && !valores[1] && !valores[3] ? verificacaoUp(14, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4202  15
      valores[2] == valores[4] && valores[2] && valores[1] && !valores[3] ? verificacaoUp(15, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0242  16
      valores[2] == valores[4] && valores[2] && !valores[1] && valores[3] ? verificacaoUp(16, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //2020  17
      valores[1] == valores[3] && valores[1] && !valores[2] && !valores[4] ? verificacaoUp(17, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2024  18
      valores[1] == valores[3] && valores[1] && !valores[2] && valores[4] && valores[3] != valores[4] ? verificacaoUp(18, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2420  19
      valores[1] == valores[3] && valores[1] && valores[2] && valores[1] != valores[2] && !valores[4] ? verificacaoUp(19, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //2002  20
      valores[1] == valores[4] && valores[1] && !valores[2] && !valores[3] ? verificacaoUp(20, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2402  21
      valores[1] == valores[4] && valores[1] && valores[2] && !valores[3] ? verificacaoUp(21, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2042  22
      valores[1] == valores[4] && valores[1] && !valores[2] && valores[3] && valores[3] != valores[4] ? verificacaoUp(22, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;

      //0002  23
      !valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoUp(23, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0020  24
      !valores[1] && !valores[2] && valores[3] && !valores[4] ? verificacaoUp(24, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0200  25
      !valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoUp(25, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2000  26
      valores[1] && !valores[2] && !valores[3] && !valores[4] ? verificacaoUp(26, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0024  27
      valores[3] != valores[4] && valores[3] && valores[4] && !valores[1] && !valores[2] ? verificacaoUp(27, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0204  28
      valores[2] != valores[4] && !valores[1] && !valores[3] && valores[2] && valores[4] ? verificacaoUp(28, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2004  29
      valores[1] != valores[4] && !valores[1] && !valores[3] && valores[1] && valores[4] ? verificacaoUp(29, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0420  30
      valores[2] != valores[3] && valores[2] && valores[3] && !valores[1] && !valores[4] ? verificacaoUp(30, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4020  31
      valores[1] != valores[3] && valores[1] && valores[3] && !valores[2] && !valores[4] ? verificacaoUp(31, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //4200  32
      valores[1] != valores[2] && valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoUp(32, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //4208  33
      valores[1] != valores[2] && valores[2] != valores[4] && valores[1]  && valores[2] && valores[4] && !valores[3] ? verificacaoUp(33, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2004  34
      valores[1] != valores[4] && valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoUp(34, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0248 35
      valores[2] != valores[3] && valores[3] != valores[4] && !valores[1] && valores[2] && valores[3] && valores[4] ? verificacaoUp(35, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2480 36
      valores[1] != valores[2] && valores[2] != valores[3] && !valores[4] && valores[1] != valores[3] &&  valores[1] && valores[2] && valores[3] ? verificacaoUp(36, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2048 37
      valores[1] != valores[3] && valores[3] != valores[4] && !valores[2] && valores[1] && valores[3] && valores[4] ? verificacaoUp(37, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;

      contadorPosition++;
    })
    gameOver();
    if(JSON.stringify(oldData) != JSON.stringify($scope.game)){
      random();
    }
  }

  $scope.down = function(){
    oldData = angular.copy($scope.game);
    let collumns = [];
    let positions = [];
    let contadorCollumns = 1;
    let contadorPosition = 1;
    let contador = 1;
    let valores = [];
    let obj;
    angular.forEach($scope.game.data, function(obj, collumn){
      collumns[contadorCollumns] = collumn;
      contadorCollumns++;
      let contadorPositions = 1;
      angular.forEach($scope.game.data[collumn], function(value, position){
        positions[contadorPositions] = position;
        contadorPositions++
      });
    });
    angular.forEach($scope.game.data, function(){
      contador = 1;
      angular.forEach($scope.game.data, function(){
      valores[contador] = $scope.game.data[collumns[contador]][positions[contadorPosition]];
      contador++;
      })
      
      //2244  1
      valores[1] == valores[2] && valores[3] == valores[4] && valores[1] && valores[3]  ? verificacaoDown(1,collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //2200  2
      valores[1] == valores[2] && valores[1] && !valores[3] && !valores[4] ? verificacaoDown(2, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2204  3
      valores[1] == valores[2] && valores[1] && !valores[3] && valores[4] ? verificacaoDown(3, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2240  4
      valores[1] == valores[2] && valores[1] && valores[3] && !valores[4] ? verificacaoDown(4, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2248  5
      valores[1] == valores[2] && valores[1] && valores[3] && valores[4] && valores[3] != valores[4] ? verificacaoDown(5, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0022  6
      valores[3] == valores[4] && valores[3] && !valores[1] && !valores[2] ? verificacaoDown(6, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0422  7
      valores[3] == valores[4] && valores[3] && !valores[1] && valores[2] ? verificacaoDown(7, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4022  8
      valores[3] == valores[4] && valores[3] && valores[1] && !valores[2] ? verificacaoDown(8, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4822  9
      valores[3] == valores[4] && valores[3] && valores[1] && valores[2] && valores[1] != valores[2] ? verificacaoDown(9, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0220  10
      valores[2] == valores[3] && valores[2] && !valores[1] && !valores[4] ? verificacaoDown(10, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4220  11
      valores[2] == valores[3] && valores[2] && valores[1] && valores[1] != valores[2] &&  !valores[4] ? verificacaoDown(11, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0224  12
      valores[2] == valores[3] && valores[2] && !valores[1] && valores[4] ? verificacaoDown(12, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4224  13
      valores[2] == valores[3] && valores[2] && valores[1] && valores[2] != valores[1] && valores[4] ? verificacaoDown(13, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;


      //0202  14
      valores[2] == valores[4] && valores[2] && !valores[1] && !valores[3] ? verificacaoDown(14, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4202  15
      valores[2] == valores[4] && valores[2] && valores[1] && !valores[3] ? verificacaoDown(15, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0242  16
      valores[2] == valores[4] && valores[2] && !valores[1] && valores[3] ? verificacaoDown(16, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //2020  17
      valores[1] == valores[3] && valores[1] && !valores[2] && !valores[4] ? verificacaoDown(17, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2024  18
      valores[1] == valores[3] && valores[1] && !valores[2] && valores[4] && valores[3] != valores[4] ? verificacaoDown(18, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2420  19
      valores[1] == valores[3] && valores[1] && valores[2] && valores[1] != valores[2] && !valores[4] ? verificacaoDown(19, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //2002  20
      valores[1] == valores[4] && valores[1] && !valores[2] && !valores[3] ? verificacaoDown(20, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2402  21
      valores[1] == valores[4] && valores[1] && valores[2] && !valores[3] ? verificacaoDown(21, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2042  22
      valores[1] == valores[4] && valores[1] && !valores[2] && valores[3] && valores[3] != valores[4] ? verificacaoDown(22, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;

      //0002  23
      !valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoDown(23, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0020  24
      !valores[1] && !valores[2] && valores[3] && !valores[4] ? verificacaoDown(24, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0200  25
      !valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoDown(25, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2000  26
      valores[1] && !valores[2] && !valores[3] && !valores[4] ? verificacaoDown(26, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0024  27
      valores[3] != valores[4] && valores[3] && valores[4] && !valores[1] && !valores[2] ? verificacaoDown(27, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0204  28
      valores[2] != valores[4] && !valores[1] && !valores[3] && valores[2] && valores[4] ? verificacaoDown(28, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2004  29
      valores[1] != valores[4] && !valores[1] && !valores[3] && valores[1] && valores[4] ? verificacaoDown(29, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //0420  30
      valores[2] != valores[3] && valores[2] && valores[3] && !valores[1] && !valores[4] ? verificacaoDown(30, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //4020  31
      valores[1] != valores[3] && valores[1] && valores[3] && !valores[2] && !valores[4] ? verificacaoDown(31, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //4200  32
      valores[1] != valores[2] && valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoDown(32, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      
      //4208  33
      valores[1] != valores[2] && valores[2] != valores[4] && valores[1]  && valores[2] && valores[4] && !valores[3] ? verificacaoDown(33, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2004  34
      valores[1] != valores[4] && valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoDown(34, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //0248 35
      valores[2] != valores[3] && valores[3] != valores[4] && !valores[1] && valores[2] && valores[3] && valores[4] ? verificacaoDown(35, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2480 36
      valores[1] != valores[2] && valores[2] != valores[3] && valores[1] != valores[3] && !valores[4] && valores[1] && valores[2] && valores[3] ? verificacaoDown(36, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;
      //2048 37
      valores[1] != valores[3] && valores[3] != valores[4] && !valores[2] && valores[1] && valores[3] && valores[4] ? verificacaoDown(37, collumns, valores[1], valores[2], valores[3], valores[4], positions[contadorPosition])  : false;

      contadorPosition++;
    })
    gameOver();
    if(JSON.stringify(oldData) != JSON.stringify($scope.game)){
      random();
    }
  }

  $scope.right = function(){
    oldData = angular.copy($scope.game);
    let contador;
    angular.forEach($scope.game.data, function(obj, collumn){
      valores = [];
      contador = 1;
      angular.forEach($scope.game.data[collumn], function(value, key){
        if(value != null){
          valores[contador] = $scope.game.data[collumn][key];
          contador++;
        }else{
          contador++;
        }
      });

      //2244  1
      valores[1] == valores[2] && valores[3] == valores[4] && valores[1] && valores[3]  ? verificacaoRight(1,collumn,  valores[1], valores[2], valores[3], valores[4])  : false;
      
      //2200  2
      valores[1] == valores[2] && valores[1] && !valores[3] && !valores[4] ? verificacaoRight(2, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2204  3
      valores[1] == valores[2] && valores[1] && !valores[3] && valores[4] ? verificacaoRight(3, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2240  4
      valores[1] == valores[2] && valores[1] && valores[3] && !valores[4] ? verificacaoRight(4, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2248  5
      valores[1] == valores[2] && valores[1] && valores[3] && valores[4] && valores[2] != valores[3] && valores[3] != valores[4] ? verificacaoRight(5, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //0022  6
      valores[3] == valores[4] && valores[3] && !valores[1] && !valores[2] ? verificacaoRight(6, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0422  7
      valores[3] == valores[4] && valores[3] && !valores[1] && valores[2] ? verificacaoRight(7, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //4022  8
      valores[3] == valores[4] && valores[3] && valores[1] && !valores[2] ? verificacaoRight(8, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //4822  9
      valores[3] == valores[4] && valores[3] && valores[1] && valores[2] && valores[1] != valores[2] ? verificacaoRight(9, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //0220  10
      valores[2] == valores[3] && valores[2] && !valores[1] && !valores[4] ? verificacaoRight(10, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //4220  11
      valores[2] == valores[3] && valores[2] && valores[1] && valores[1] != valores[2] && !valores[4] ? verificacaoRight(11, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0224  12
      valores[2] == valores[3] && valores[2] && !valores[1] && valores[4] ? verificacaoRight(12, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //4224  13
      valores[2] == valores[3] && valores[2] && valores[1] && valores[4] && valores[2] != valores[1] ? verificacaoRight(13, collumn, valores[1], valores[2], valores[3], valores[4])  : false;


      //0202  14
      valores[2] == valores[4] && valores[2] && !valores[1] && !valores[3] ? verificacaoRight(14, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //4202  15
      valores[2] == valores[4] && valores[2] && valores[1] && !valores[3] ? verificacaoRight(15, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0242  16
      valores[2] == valores[4] && valores[2] && !valores[1] && valores[3] ? verificacaoRight(16, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //2020  17
      valores[1] == valores[3] && valores[1] && !valores[2] && !valores[4] ? verificacaoRight(17, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2024  18
      valores[1] == valores[3] && valores[1] && !valores[2] && valores[4] && valores[3] != valores[4] ? verificacaoRight(18, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2420  19
      valores[1] == valores[3] && valores[1] && valores[2]  && valores[1] != valores[2] && !valores[4] ? verificacaoRight(19, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //2002  20
      valores[1] == valores[4] && valores[1] && !valores[2] && !valores[3] ? verificacaoRight(20, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2402  21
      valores[1] == valores[4] && valores[1] && valores[2] && !valores[3] ? verificacaoRight(21, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2042  22
      valores[1] == valores[4] && valores[1] && !valores[2] && valores[3] && valores[3] != valores[4] ? verificacaoRight(22, collumn, valores[1], valores[2], valores[3], valores[4])  : false;

      //0002  23
      !valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoRight(23, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0020  24
      !valores[1] && !valores[2] && valores[3] && !valores[4] ? verificacaoRight(24, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0200  25
      !valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoRight(25, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2000  26
      valores[1] && !valores[2] && !valores[3] && !valores[4] ? verificacaoRight(26, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //0024  27
      valores[3] != valores[4] && valores[3] && valores[4] && !valores[1] && !valores[2] ? verificacaoRight(27, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0204  28
      valores[2] != valores[4] && !valores[1] && !valores[3] && valores[2] && valores[4] ? verificacaoRight(28, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2004  29
      valores[1] != valores[4] && !valores[1] && !valores[3] && valores[1] && valores[4] ? verificacaoRight(29, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //0420  30
      valores[2] != valores[3] && valores[2] && valores[3] && !valores[1] && !valores[4] ? verificacaoRight(30, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //4020  31
      valores[1] != valores[3] && valores[1] && valores[3] && !valores[2] && !valores[4] ? verificacaoRight(31, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //4200  32
      valores[1] != valores[2] && valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoRight(32, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      
      //4208  33
      valores[1] != valores[2] && valores[2] != valores[4] && valores[1]  && valores[2] && valores[4] && !valores[3] ? verificacaoRight(33, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2004  34
      valores[1] != valores[4] && valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoRight(34, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //0248 35
      valores[2] != valores[3] && valores[3] != valores[4] && !valores[1] && valores[2] && valores[3] && valores[4] ? verificacaoRight(35, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2480 36
      valores[1] != valores[2] && valores[2] != valores[3] && valores[1] != valores[3] && !valores[4] && valores[1] && valores[2] && valores[3] ? verificacaoRight(36, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
      //2048 37
      valores[1] != valores[3] && valores[3] != valores[4] && !valores[2] && valores[1] && valores[3] && valores[4] ? verificacaoRight(37, collumn, valores[1], valores[2], valores[3], valores[4])  : false;


    });
    gameOver();
    if(JSON.stringify(oldData) != JSON.stringify($scope.game)){
      random();
    }
  }
 
  $scope.left = function(){
    oldData = angular.copy($scope.game);
      let contador = 1;
    angular.forEach($scope.game.data, function(obj, collumn){
      valores = [];
      contador = 1;
      angular.forEach($scope.game.data[collumn], function(value, key){
        if(value != null){
          valores[contador] = $scope.game.data[collumn][key];
          contador++;
        }else{
          contador++;
        }
      });
      
        //2244  1
        valores[1] == valores[2] && valores[3] == valores[4] && valores[1] && valores[3]  ? verificacaoLeft(1,collumn,  valores[1], valores[2], valores[3], valores[4])  : false;
        
        //2200  2
        valores[1] == valores[2] && valores[1] && !valores[3] && !valores[4] ? verificacaoLeft(2, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2204  3
        valores[1] == valores[2] && valores[1] && !valores[3] && valores[4] ? verificacaoLeft(3, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2240  4
        valores[1] == valores[2] && valores[1] && valores[3] && !valores[4] ? verificacaoLeft(4, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2248  5
        valores[1] == valores[2] && valores[1] && valores[3] && valores[4] && valores[2] != valores[3] && valores[3] != valores[4] ? verificacaoLeft(5, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //0022  6
        valores[3] == valores[4] && valores[3] && !valores[1] && !valores[2] ? verificacaoLeft(6, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0422  7
        valores[3] == valores[4] && valores[3] && !valores[1] && valores[2] ? verificacaoLeft(7, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //4022  8
        valores[3] == valores[4] && valores[3] && valores[1] && !valores[2] ? verificacaoLeft(8, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //4822  9
        valores[3] == valores[4] && valores[3] && valores[1] && valores[2] && valores[1] != valores[2] ? verificacaoLeft(9, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //0220  10
        valores[2] == valores[3] && valores[2] && !valores[1] && !valores[4] ? verificacaoLeft(10, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //4220  11
        valores[2] == valores[3] && valores[2] && valores[1] && valores[1] != valores[2] && !valores[4] ? verificacaoLeft(11, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0224  12
        valores[2] == valores[3] && valores[2] && !valores[1] && valores[4] ? verificacaoLeft(12, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //4224  13
        valores[2] == valores[3] && valores[2] && valores[1] && valores[4] && valores[2] != valores[1] ? verificacaoLeft(13, collumn, valores[1], valores[2], valores[3], valores[4])  : false;


        //0202  14
        valores[2] == valores[4] && valores[2] && !valores[1] && !valores[3] ? verificacaoLeft(14, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //4202  15
        valores[2] == valores[4] && valores[2] && valores[1] && !valores[3] ? verificacaoLeft(15, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0242  16
        valores[2] == valores[4] && valores[2] && !valores[1] && valores[3] ? verificacaoLeft(16, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //2020  17
        valores[1] == valores[3] && valores[1] && !valores[2] && !valores[4] ? verificacaoLeft(17, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2024  18
        valores[1] == valores[3] && valores[1] && !valores[2] && valores[4] && valores[3] != valores[4] ? verificacaoLeft(18, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2420  19
        valores[1] == valores[3] && valores[1] && valores[2] && valores[1] != valores[2] && !valores[4] ? verificacaoLeft(19, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //2002  20
        valores[1] == valores[4] && valores[1] && !valores[2] && !valores[3] ? verificacaoLeft(20, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2402  21
        valores[1] == valores[4] && valores[1] && valores[2] && !valores[3] ? verificacaoLeft(21, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2042  22
        valores[1] == valores[4] && valores[1] && !valores[2] && valores[3] && valores[3] != valores[4] ? verificacaoLeft(22, collumn, valores[1], valores[2], valores[3], valores[4])  : false;

        //0002  23
        !valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoLeft(23, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0020  24
        !valores[1] && !valores[2] && valores[3] && !valores[4] ? verificacaoLeft(24, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0200  25
        !valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoLeft(25, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2000  26
        valores[1] && !valores[2] && !valores[3] && !valores[4] ? verificacaoLeft(26, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //0024  27
        valores[3] != valores[4] && valores[3] && valores[4] && !valores[1] && !valores[2] ? verificacaoLeft(27, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0204  28
        valores[2] != valores[4] && !valores[1] && !valores[3] && valores[2] && valores[4] ? verificacaoLeft(28, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2004  29
        valores[1] != valores[4] && !valores[1] && !valores[3] && valores[1] && valores[4] ? verificacaoLeft(29, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //0420  30
        valores[2] != valores[3] && valores[2] && valores[3] && !valores[1] && !valores[4] ? verificacaoLeft(30, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //4020  31
        valores[1] != valores[3] && valores[1] && valores[3] && !valores[2] && !valores[4] ? verificacaoLeft(31, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //4200  32
        valores[1] != valores[2] && valores[1] && valores[2] && !valores[3] && !valores[4] ? verificacaoLeft(32, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        
        //4208  33
        valores[1] != valores[2] && valores[2] != valores[4] && valores[1]  && valores[2] && valores[4] && !valores[3] ? verificacaoLeft(33, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2004  34
        valores[1] != valores[4] && valores[1] && !valores[2] && !valores[3] && valores[4] ? verificacaoLeft(34, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //0248  35
        valores[2] != valores[3] && valores[3] != valores[4] && !valores[1] && valores[2] && valores[3] && valores[4] ? verificacaoLeft(35, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2480 36
        valores[1] != valores[2] && valores[2] != valores[3] && valores[1] != valores[3] && !valores[4] && valores[1] && valores[2] && valores[3] ? verificacaoLeft(36, collumn, valores[1], valores[2], valores[3], valores[4])  : false;
        //2048 37
        valores[1] != valores[3] && valores[3] != valores[4] && !valores[2] && valores[1] && valores[3] && valores[4] ? verificacaoLeft(37, collumn, valores[1], valores[2], valores[3], valores[4])  : false;

    });
    gameOver();
    if(JSON.stringify(oldData) != JSON.stringify($scope.game)){
      random();
    }
  }

  function verificacaoUp(keyValue, collumn, posicao1, posicao2, posicao3, posicao4, value){
    switch(keyValue){
      case 1:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = posicao3*2;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 2:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = null;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 3:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = posicao4;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 4:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = posicao3;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 5:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = posicao3;
        $scope.game.data[collumn[3]][value] = posicao4;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 6:
        $scope.game.data[collumn[1]][value] = posicao3*2;
        $scope.game.data[collumn[2]][value] = null;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 7:
        $scope.game.data[collumn[1]][value] = posicao2;
        $scope.game.data[collumn[2]][value] = posicao3*2;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 8:
        $scope.game.data[collumn[1]][value] = posicao1;
        $scope.game.data[collumn[2]][value] = posicao3*2;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 9:
        $scope.game.data[collumn[1]][value] = posicao1;
        $scope.game.data[collumn[2]][value] = posicao2;
        $scope.game.data[collumn[3]][value] = posicao3*2;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 10:
        $scope.game.data[collumn[1]][value] = posicao2*2;
        $scope.game.data[collumn[2]][value] = null;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 11:
        $scope.game.data[collumn[1]][value] = posicao1;
        $scope.game.data[collumn[2]][value] = posicao2*2;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 12:
        $scope.game.data[collumn[1]][value] = posicao2*2;
        $scope.game.data[collumn[2]][value] = posicao4;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 13:
        $scope.game.data[collumn[1]][value] = posicao1;
        $scope.game.data[collumn[2]][value] = posicao2*2;
        $scope.game.data[collumn[3]][value] = posicao4;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 14:
        $scope.game.data[collumn[1]][value] = posicao2*2;
        $scope.game.data[collumn[2]][value] = null;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 15:
        $scope.game.data[collumn[1]][value] = posicao1;
        $scope.game.data[collumn[2]][value] = posicao2*2;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 16:
        $scope.game.data[collumn[1]][value] = posicao2;
        $scope.game.data[collumn[2]][value] = posicao3;
        $scope.game.data[collumn[3]][value] = posicao4;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 17:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = null;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 18:
        $scope.game.data[collumn[1]][value] = posicao1*2;
        $scope.game.data[collumn[2]][value] = posicao4;
        $scope.game.data[collumn[3]][value] = null;
        $scope.game.data[collumn[4]][value] = null;
      break;
      case 19:
        $scope.game.data[collumn[1]][value] = posicao1;
        $scope.game.data[collumn[2]][value] = posicao2;
