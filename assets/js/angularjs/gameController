        $scope.game.data[collumn]['value4'] = null;
      break;
      case 30:
        $scope.game.data[collumn]['value1'] = posicao2;
        $scope.game.data[collumn]['value2'] = posicao3;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 31:
