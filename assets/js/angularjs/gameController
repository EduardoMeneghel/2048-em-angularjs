  function verificacaoDown(keyValue, collumn, posicao1, posicao2, posicao3, posicao4, value){
    switch(keyValue){
      case 1:
        $scope.game.data[collumn[1]][value] = null;
        $scope.game.data[collumn[2]][value] = null;
        $scope.game.data[collumn[3]][value] = posicao1*2;
        $scope.game.data[collumn[4]][value] = posicao3*2;
