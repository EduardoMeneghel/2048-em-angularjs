  function gameOver(){
    $scope.game.data['column1']['value1'] && $scope.game.data['column1']['value2'] && $scope.game.data['column1']['value3'] && $scope.game.data['column1']['value4'] &&
    $scope.game.data['column2']['value1'] && $scope.game.data['column2']['value2'] && $scope.game.data['column2']['value3'] && $scope.game.data['column2']['value4'] &&
    $scope.game.data['column3']['value1'] && $scope.game.data['column3']['value2'] && $scope.game.data['column3']['value3'] && $scope.game.data['column3']['value4'] &&
    $scope.game.data['column4']['value1'] && $scope.game.data['column4']['value2'] && $scope.game.data['column4']['value3'] && $scope.game.data['column4']['value4'] &&

    $scope.game.data['column1']['value1'] != $scope.game.data['column1']['value2'] && $scope.game.data['column1']['value2'] != $scope.game.data['column1']['value3'] && $scope.game.data['column1']['value3'] != $scope.game.data['column1']['value4'] &&
    $scope.game.data['column2']['value1'] != $scope.game.data['column2']['value2'] && $scope.game.data['column2']['value2'] != $scope.game.data['column2']['value3'] && $scope.game.data['column2']['value3'] != $scope.game.data['column2']['value4'] &&
    $scope.game.data['column3']['value1'] != $scope.game.data['column3']['value2'] && $scope.game.data['column3']['value2'] != $scope.game.data['column3']['value3'] && $scope.game.data['column3']['value3'] != $scope.game.data['column3']['value4'] &&
    $scope.game.data['column4']['value1'] != $scope.game.data['column4']['value2'] && $scope.game.data['column4']['value2'] != $scope.game.data['column4']['value3'] && $scope.game.data['column4']['value3'] != $scope.game.data['column4']['value4'] &&

    $scope.game.data['column1']['value1'] != $scope.game.data['column2']['value1'] && $scope.game.data['column2']['value1'] != $scope.game.data['column3']['value1'] && $scope.game.data['column3']['value1'] != $scope.game.data['column4']['value1'] &&
    $scope.game.data['column1']['value2'] != $scope.game.data['column2']['value2'] && $scope.game.data['column2']['value2'] != $scope.game.data['column3']['value2'] && $scope.game.data['column3']['value2'] != $scope.game.data['column4']['value2'] &&
    $scope.game.data['column1']['value3'] != $scope.game.data['column2']['value3'] && $scope.game.data['column2']['value3'] != $scope.game.data['column3']['value3'] && $scope.game.data['column3']['value3'] != $scope.game.data['column4']['value3'] &&
    $scope.game.data['column1']['value4'] != $scope.game.data['column2']['value4'] && $scope.game.data['column2']['value4'] != $scope.game.data['column3']['value4'] && $scope.game.data['column3']['value4'] != $scope.game.data['column4']['value4']
     ? alert('Jogo Acabou') : false;
  }
    

  function random(){
      let ramdomCollumn = [];
      let ramdomKey = [];
      let contador = 0;
      let numberRandom;
      angular.forEach($scope.game.data, function(obj, collumn){
        angular.forEach($scope.game.data[collumn], function(value, key){
          if(value == null){
            ramdomCollumn[contador] = collumn;
            ramdomKey[contador] = key;
            contador++;
          }
        });
      });
      if(contador){
        numberRandom = Math.floor(Math.random() * ramdomCollumn.length);
        $scope.game.data[ramdomCollumn[numberRandom]][ramdomKey[numberRandom]] = 2;
        color()
      }
      color()
  }

  $scope.reset = function() {
    $scope.game = {
      "data" : {
        "column1": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
        "column2": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
        "column3": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
        "column4": {
          value1:null,
          value2:null,
          value3:null,
          value4:null
        },
      },
      "color" : {
          "column1": {
            value1:null,
            value2:null,
            value3:null,
            value4:null
          },
          "column2": {
            value1:null,
            value2:null,
            value3:null,
            value4:null
          },
          "column3": {
            value1:null,
            value2:null,
            value3:null,
            value4:null
          },
          "column4": {
            value1:null,
            value2:null,
            value3:null,
            value4:null
        },
      }
    }
    random();
    random();
  }

  function color(){
    angular.forEach($scope.game.data, function(obj, collumn){
      angular.forEach($scope.game.data[collumn], function(element, value){
        switch(element){
          case 2:
            $scope.game.color[collumn][value] = 'Gold';
            break;
          case 4:
            $scope.game.color[collumn][value] = 'DarkOrange';
            break;
          case 8:
            $scope.game.color[collumn][value] = 'DodgerBlue';
            break;
          case 16:
            $scope.game.color[collumn][value] = 'DeepSkyBlue';
            break;
          case 32:
            $scope.game.color[collumn][value] = 'DeepPink';
            break;
          case 64:
            $scope.game.color[collumn][value] = 'LawnGreen';
            break;
          case 128:
            $scope.game.color[collumn][value] = 'Orchid';
            break;
          case 256:
            $scope.game.color[collumn][value] = 'MediumPurple';
            break;
          case 512:
            $scope.game.color[collumn][value] = 'DeepPink';
            break;
          case 1024:
            $scope.game.color[collumn][value] = 'DodgerBlue';
            break;
          case 2048:
            $scope.game.color[collumn][value] = 'BlueViolet';
            break;
          default:
            $scope.game.color[collumn][value] = 'Gray';
            break;
        }
      });
    });
  }
});
})(window.angular);