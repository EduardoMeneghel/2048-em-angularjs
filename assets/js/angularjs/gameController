  function verificacaoLeft(keyValue, collumn, posicao1, posicao2,posicao3,posicao4){
    switch(keyValue){
      case 1:
        $scope.game.data[collumn]['value1'] = posicao1*2;
        $scope.game.data[collumn]['value2'] = posicao3*2;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 2:
        $scope.game.data[collumn]['value1'] = posicao1*2;
        $scope.game.data[collumn]['value2'] = null;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 3:
        $scope.game.data[collumn]['value1'] = posicao1*2;
        $scope.game.data[collumn]['value2'] = posicao4;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 4:
        $scope.game.data[collumn]['value1'] = posicao1*2;
        $scope.game.data[collumn]['value2'] = posicao3;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 5:
        $scope.game.data[collumn]['value1'] = posicao1*2;
        $scope.game.data[collumn]['value2'] = posicao3;
        $scope.game.data[collumn]['value3'] = posicao4;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 6:
        $scope.game.data[collumn]['value1'] = posicao3*2;
        $scope.game.data[collumn]['value2'] = null;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 7:
        $scope.game.data[collumn]['value1'] = posicao2;
        $scope.game.data[collumn]['value2'] = posicao3*2;
        $scope.game.data[collumn]['value3'] = null;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 8:
