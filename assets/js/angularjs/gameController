        $scope.game.data[collumn]['value4'] = null;
      break;
      case 33:
        $scope.game.data[collumn]['value1'] = posicao1;
        $scope.game.data[collumn]['value2'] = posicao2;
        $scope.game.data[collumn]['value3'] = posicao4;
        $scope.game.data[collumn]['value4'] = null;
      break;
      case 34:
